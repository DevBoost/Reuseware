       uri = "http://www.reuseware.org/application/taipan" 
       class = "org.reuseware.application.taipan.TaiPanPackage"
       genModel = "models/taipan.genmodel" /> 
  </extension>

   <extension
         point="org.reuseware.sokan.resource.pluginStore">
