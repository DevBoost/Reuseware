      	category="org.reuseware.coconut.ui/org.reuseware.coconut.ui.examples" 
      	class="org.reuseware.application.taipan.example.taosd.ExampleUnzipperNewWizard" 
      	project="true" id="org.modelplex.cim.tidlibrary">
